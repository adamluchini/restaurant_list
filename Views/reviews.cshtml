<html>
<head>
  <link href="../Content/CSS/bootstrap.css" rel="stylesheet">
  <link href="../Content/CSS/styles.css" rel="stylesheet">
  <meta charset="utf-8">
</head>
<body>
<h2>Here are all the reviews for this restaurant</h2>



  <ol>
  @foreach (var review in @Model)
    {
    <li>@review.GetDescription()</li>
    }
  </ol>
<p><a href="/reviews/new">Add a new review</a></p>
<p><a href = "/">Return home</a></p>
</body>
</html>
